{
   "name": "arthega-asdweri/phoenix-php",
   "type": "library",
   "description": "Simple and fast PHP Framework with Vue",
   "keywords": [
      "php",
      "framework",
      "vue"
   ],
   "homepage": "https://www.phoenix-php.org",
   "authors": [
      {
         "name": "Daniel Hahn",
         "email": "daniel@hahn-webdesign.de"
      }
   ],
   "require": {
      "php": ">=8.1.0",
      "symfony/yaml": "6.*"
   },
   "require-dev": {
      "phpunit/phpunit": "^9",
      "phpstan/phpstan": "^1",
      "psr/log": "3.0.0"
   },
   "autoload": {
      "psr-4": {
         "PhoenixPhp\\": "src"
      }
   },
   "autoload-dev": {
      "psr-4": {
         "PhoenixPhp\\": "tests"
      }
   },
   "license": "GPL-3.0+"
}
